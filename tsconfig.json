{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "strict": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/apps/*": [
        "src/apps/*"
      ],
      "@/system/*": [
        "src/system/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/contexts/*": [
        "src/contexts/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/themes/*": [
        "themes/*"
      ]
    },
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/utils/notion.js",
    "src/utils/notion.js",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ]
}
